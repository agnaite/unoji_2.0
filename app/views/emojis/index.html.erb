<div class="container">
  <div class="unoji_group">
    <% count = 0 %>
    <div id='popup'>Copied!</div>
    <!-- <% sample = @emojis.sample(10) %> -->
    <% @emojis.each do |emoji| %>
      <%= link_to emoji.face, {:controller => "emojis", :action => "click", :unoji => emoji.id }, remote: true, class: "unoji col-md-2 col-sm-4 col-xs-4 face_#{count}", :"data-clipboard-target" => :".face_#{count}" %>
      <% count += 1 %>
    <% end %>
  </div>
</div>
